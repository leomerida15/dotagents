{
	"private": true,
	"type": "module",
	"name": "@dotagents/diff",
	"module": "dist/index.js",
	"exports": {
		".": {
			"import": "./dist/index.js",
			"types": "./dist/index.d.ts"
		}
	},
	"scripts": {
		"build": "bun run -b bunstart.build.ts",
		"watch": "bun run -b bunstart.watch.ts",
		"dev": "bun run watch",
		"start": "bun src/index.ts",
		"test": "vitest run",
		"test:integration": "vitest run --testPathPattern=integration",
		"types:check": "bun run -b tsc --noEmit"
	},
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"version": "0.0.1",
	"devDependencies": {
		"@types/bun": "latest",
		"vitest": "^1.2.0"
	},
	"peerDependencies": {
		"typescript": "^5"
	}
}